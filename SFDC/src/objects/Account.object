        <errorConditionFormula>OR(AND( Success_of_a_Business_Deal__c =true, Business_Deal_Details__c=&quot;&quot;),AND( Success_of_a_Business_Deal__c =FALSE, Business_Deal_Details__c&lt;&gt;&quot;&quot;))</errorConditionFormula>
