    <validationRules>
        <fullName>Account_Type_Validation_Rule</fullName>
        <active>false</active>
        <errorConditionFormula>AND( ISPICKVAL( Type , &quot;targets&quot;) , ISBLANK( Rating__c ))</errorConditionFormula>
        <errorMessage>Make &quot;Rating&quot; field active only when the Account Type is ‘Target’</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Anniversary_validation</fullName>
        <active>false</active>
        <errorConditionFormula>OR(AND( Wedding_Anniversary__c =true, Wedding_Anniversary_Details__c =&quot;&quot;),AND( Wedding_Anniversary__c =FALSE, Wedding_Anniversary_Details__c &lt;&gt;&quot;&quot;))</errorConditionFormula>
        <errorMessage>Wedding Anniversary Details Error</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Birthday_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>OR( AND ( Birthday__c =false, Birthday_Details__c &lt;&gt; &quot;&quot;), AND ( Birthday__c = true, Birthday_Details__c = &quot;&quot;))</errorConditionFormula>
        <errorMessage>Birthday details error</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ChildBirth_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>OR(AND ( Childbday_Prg__c   = true,  Childbirth_Pregnancy_dtls__c   = &quot;&quot;),AND ( Childbday_Prg__c   = FALSE,  Childbirth_Pregnancy_dtls__c   &lt;&gt; &quot;&quot;))</errorConditionFormula>
        <errorMessage>Childbirth/Pregnancy Details Error</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Diwali_dashera_Validation</fullName>
        <active>false</active>
        <description>Diwali Dashera Error</description>
        <errorConditionFormula>OR(AND (  Diwali_Dushera__c   = true,   Diwali_Dushera_dtls__c   = &quot;&quot;),AND (  Diwali_Dushera__c   = FALSE,   Diwali_Dushera_dtls__c   &lt;&gt;&quot;&quot;))</errorConditionFormula>
        <errorMessage>Diwali or Dashera Details Error</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Festival_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>OR(AND ( Festivals__c    = true,   Festival_Information_dtls__c    = &quot;&quot;),AND ( Festivals__c    = FALSE,   Festival_Information_dtls__c    &lt;&gt; &quot;&quot;))</errorConditionFormula>
        <errorMessage>Festival Details Error</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Inheritance_validation</fullName>
        <active>false</active>
        <description>Inheritance Error</description>
        <errorConditionFormula>OR(AND( Inheritance__c =true, Inheritance_details__c =&quot;&quot;),AND( Inheritance__c =FALSE, Inheritance_details__c &lt;&gt;&quot;&quot;))</errorConditionFormula>
        <errorMessage>Inheritance details Error</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Occasions_Validation</fullName>
        <active>false</active>
        <description>Other occasions Details Error</description>
        <errorConditionFormula>OR(AND ( Other_Occasions1__c     = true,   Other_Occasions_dtls__c  = &quot;&quot;),AND ( Other_Occasions1__c     = FALSE,   Other_Occasions_dtls__c  &lt;&gt; &quot;&quot;))</errorConditionFormula>
        <errorMessage>Other occasions Details Error</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Propensitytospend_validation</fullName>
        <active>false</active>
        <description>spending Capatity details Error</description>
        <errorConditionFormula>OR(AND( Propensity_to_Spend1__c =true, Spending_Capacity__c =&quot;&quot;),AND( Propensity_to_Spend1__c =FALSE, Spending_Capacity__c &lt;&gt;&quot;&quot;))</errorConditionFormula>
